<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".PantallaPrincipal">
    <ImageView
            android:layout_width="664dp"
            android:layout_height="933dp" app:srcCompat="@drawable/fondo"
            android:id="@+id/imageView5" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:contentDescription="@string/imagen2"
            android:background="#0C0A0A"/>
    <FrameLayout
            android:layout_width="413dp"
            android:layout_height="65dp"
            android:background="#E2D9D9" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0">

        <TextView
                android:text="@string/ctrganadero"
                android:translationY="15sp"
                android:layout_width="269dp"
                android:layout_height="wrap_content" android:id="@+id/textView2" android:textAlignment="center"
                android:lineSpacingExtra="200dp" android:textSize="25sp" android:fontFamily="sans-serif-black"
                android:textStyle="bold"/>
        <ImageView
                android:layout_width="72dp"
                android:translationX="275sp"
                android:layout_height="63dp" app:srcCompat="@drawable/vacamas" android:id="@+id/imageView6"
                android:contentDescription="@string/vacamas2"/>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>